<iscontent type="text/html" charset="UTF-8" />
<isdecorate template="application/MenuFrame">
  <link rel="stylesheet" type="text/css" href="${URLUtils.staticURL('./css/invoice.css')}" />
  <div class="headline">${Resource.msg('invoice.heading','invoice',null)}</div>
  <form id="downloadinvoice" action="${URLUtils.url('Download-Invoice')}">
