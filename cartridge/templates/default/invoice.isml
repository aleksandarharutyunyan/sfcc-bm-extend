          <input type="text" id="filename" name="filename" placeholder="Enter Invoice/CreditMemo FileName..." />
          <button id="save-all-preferences" type="submit">${Resource.msg('invoice.button','invoice',null)}</button>
      </div>
  </form>
</isdecorate>